if 5 > 2:
    print("Five is greater than two!")
#Let's try casting some Variables
x= str(3) #'3'
y= int(3) #3
z= float(3) #3.0
print (x, y, z)
print (type(x)) #Get the type! str
print(type(y)) #check out y as well, int
print(type(z)) #now z! float

fruits = ["Apple", "Banana", "Cherry"]
fruitApple , fruitBanana, fruitCherry = fruits
print(fruitApple, fruitBanana, fruitCherry)

x = "Awesome"
def myfunc():
    global x
    x = "Fantastic!"
    print("Python is " + x)

myfunc()
print("Pythons " + x)

#Python numbers
x = 1 # int, whole number without +/-, decimal, unlim. length
y = 2.8 # float, positive or negative, one or more decimals
z = 1j # complex 

#Type conversion
a = float(x)
b= int(y)
c = complex (x)

print(a, b, c)
print(type(a))
print(type(b))
print(type(c))

import random
print(random.randrange(1, 69))